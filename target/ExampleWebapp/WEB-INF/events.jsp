</form:select>
<form:select path="year">
    <option disabled selected value> - Year - </option>
    <option value="2016">2016</option>
    <option value="2017">2017</option>
    <option value="2018">2018</option>
</form:select>
<input type="submit" value="Submit Event">
 </form:form>
</body>
</html>
